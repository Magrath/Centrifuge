		function show (vid) { 
		p = document.createElement ('P') 
		strong = document.createElement ('STRONG') 
		p.appendChild (strong) 
		strong.appendChild (document.createTextNode ('code for video')) 
		strong.appendChild (document.createTextNode (vid)) 
		br = document.createElement ('BR') 
		p.appendChild (br) 
		p.appendChild (document.createTextNode ('it works! yay!')) 
		document.getElementById ('video'+vid).appendChild (p)
		} 
</script> <style type="text/css"> label {display:block} </style> </head> <body> <div> 


